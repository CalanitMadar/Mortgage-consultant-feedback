{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "SUPABASE_URL": "@your-supabase-url",
      "SUPABASE_KEY": "@your-supabase-key"
    }
  }
  